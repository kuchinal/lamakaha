addUserKnob {7 scaleY l "scale Y" R 1 100}
scaleY 1
addUserKnob {26 "" +STARTLINE}
addUserKnob {7 blackpoint R -1 1}
addUserKnob {7 whitepoint R 0 2}
whitepoint 1
}
Input {
inputs 0
name WorldP
xpos -918
ypos 363
}
Grade {
white {{1/parent.scale_1/100}}
black_clamp false
name Grade231
