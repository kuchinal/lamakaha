set cut_paste_input [stack 0]
version 9.0 v7
push $cut_paste_input
Group {
name Pnoise_Blocky
help "Apply noise on your UV/Normal/WorldPosPass\nexpecting RGB"
autolabel "nuke.thisNode()\['label'].value()"
tile_color 0x695287ff
note_font "Bitstream Vera Sans Bold"
note_font_size 20
selected true
xpos 5316
ypos 796
addUserKnob {20 User}
addUserKnob {7 scale_1 l scale R 0 10}
scale_1 1
addUserKnob {6 scale_panelDropped l "panel dropped state" -STARTLINE +HIDDEN}
